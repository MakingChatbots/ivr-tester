{
  "name": "Keys pressed are read back",
  "steps": [
    {
      "whenPrompt": {
        "type": "isAnything"
      },
      "then": {
        "type": "press",
        "value": "1"
      },
      "silenceAfterPrompt": 2000,
      "timeout": 6000
    },
    {
      "whenPrompt": {
        "type": "contains",
        "value": "please enter a number"
      },
      "then": {
        "type": "press",
        "value": "0123456789"
      },
      "silenceAfterPrompt": 3000,
      "timeout": 6000
    },
    {
      "whenPrompt": {
        "type": "contains",
        "value": "you entered the values 0123456789"
      },
      "then": {
        "type": "hangUp"
      },
      "silenceAfterPrompt": 3000,
      "timeout": 6000
    }
  ]
}
