{
  "name": "ivr-tester-google-speech-to-text",
  "version": "0.0.1",
  "description": "Google Speech-to-Text integration for IVR Tester",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SketchingDev/ivr-tester.git"
  },
  "homepage": "https://github.com/SketchingDev/ivr-tester#ivr-tester",
  "keywords": ["ivr", "call flow", "test", "google", "speech-to-text"],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test:integration": "jest __tests__",
    "clean": "rimraf dist/",
    "build": "yarn clean && tsc --declaration",
    "lint:prettier": "prettier --list-different '{src,__tests__}/**/*.ts' --write",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint": "yarn lint:prettier && yarn lint:eslint",
    "prepare" : "yarn build"
  },
  "files": [
    "/dist"
  ],
  "dependencies": {
    "@google-cloud/speech": "^4.1.3"
  },
  "devDependencies": {
    "@types/getenv": "^1.0.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.6.4",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-jest": "^24.0.2",
    "jest": "^26.4.2",
    "jest-when": "^2.7.2",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "wait-for-expect": "^3.0.2",
    "ivr-tester": "0.x"
  },
  "peerDependencies": {
    "ivr-tester": "0.x"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">= 10.14.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
