{
  "name": "ivr-tester-cli",
  "version": "0.0.1",
  "description": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SketchingDev/ivr-tester.git"
  },
  "homepage": "https://github.com/SketchingDev/ivr-tester#ivr-tester",
  "keywords": [
    "IVR",
    "Interactive Voice Response",
    "call flow",
    "call",
    "test",
    "telephony"
  ],
  "main": "dist/index.js",
  "bin": {
    "ivr-tester": "./dist/index.js"
  },
  "scripts": {
    "execute": "node --require ts-node/register src/index.ts",
    "test": "jest src",
    "test:integration": "jest '/integration-tests'",
    "clean": "rimraf dist/",
    "build": "yarn clean && tsc -p tsconfig.build.json --declaration",
    "lint:prettier": "prettier --list-different 'src/**/*.ts' --write",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint": "yarn lint:prettier && yarn lint:eslint",
    "prepare": "yarn build"
  },
  "files": [
    "/dist"
  ],
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^7.2.0",
    "debug": "^4.3.1",
    "getenv": "^1.0.0",
    "ivr-tester": "^0.2.10",
    "ivr-tester-transcriber-google-speech-to-text": "^0.1.3",
    "joi": "^17.4.0",
    "ngrok": "^3.4.0"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "^7.0.2",
    "@types/debug": "^4.1.5",
    "@types/getenv": "^1.0.0",
    "@types/jest": "^26.0.14",
    "@types/jest-when": "^2.7.2",
    "@types/node": "^14.6.4",
    "copyfiles": "^2.4.0",
    "get-port": "^5.1.1",
    "jest": "^26.4.2",
    "jest-when": "^3.3.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "wait-for-expect": "^3.0.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">= 10.14.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
